  "keywords": [
    "nodejs",
    "typescript",
    "backend"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@types/mongoose": "^5.11.96",
    "express": "^4.18.2",
    "mongoose": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
